  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "prepare:ci": "npm run build -w @nou/web && npm run db:push -w @nou/web && npm run db:seed -w @nou/web",
    "build:web": "dotenvx run --env-file=.env.test -- npm run build -w @nou/web",
    "db:push": "dotenvx run --env-file=.env.test -- npm run db:push -w @nou/web",
    "db:seed": "dotenvx run --env-file=.env.test -- npm run db:seed -w @nou/web -- --seed-file=$INIT_CWD/.drizzle/seed.ts",
    "db:setup": "npm run db:push && npm run db:seed",
    "start:web": "dotenvx run --env-file=.env.test -- npm run dev -w @nou/web",
    "typecheck": "tsc",
    "lint": "eslint src",
    "format": "prettier . --write"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.14.2"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.44.1",
    "@playwright/test": "^1.44.1"
  }
