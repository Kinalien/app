{
	"name": "@nou/web",
	"version": "0.0.1",
	"type": "module",
	"license": "GNU General Public License v3.0",
	"scripts": {
		"dev": "vinxi dev --host -- --env-file=.env",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/i18n/runtime && vinxi build",
		"start": "vinxi start",
		"typecheck": "tsc",
		"check": "npx @biomejs/biome check .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "npx tsx .drizzle/migrate.ts",
		"db:seed": "npx tsx .drizzle/seed.ts",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio --port=3001"
	},
	"dependencies": {
		"@nou/ui": "^0.0.1",
		"@solid-primitives/i18n": "^2.1.1",
		"@solid-primitives/refs": "^1.0.8",
		"@solid-primitives/storage": "^4.2.1",
		"@solidjs/meta": "^0.29.4",
		"@solidjs/router": "^0.15.1",
		"@solidjs/start": "^1.0.10",
		"arctic": "^2.2.2",
		"drizzle-orm": "^0.36.1",
		"nanoid": "^5.0.8",
		"oslo": "^1.2.1",
		"pg": "^8.13.1",
		"solid-js": "^1.9.3",
		"tailwind-variants": "^0.2.1",
		"temporal-polyfill": "^0.2.5",
		"valibot": "^1.0.0-beta.3",
		"vinxi": "0.4.3"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.2.0",
		"@nou/config": "^1.0.0",
		"@storybook/builder-vite": "^8.4.2",
		"@types/pg": "^8.11.10",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.28.0",
		"tailwindcss": "^3.4.14",
		"tsx": "^4.19.2",
		"vite": "^5.4.11",
		"vite-imagetools": "^7.0.4",
		"vite-svg-sprite-wrapper": "^1.3.3",
		"vite-tsconfig-paths": "^5.1.2"
	},
	"engines": {
		"node": ">=22"
	}
}
